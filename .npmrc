# Configuration NPM pour résoudre les conflits de dépendances
legacy-peer-deps=true
auto-install-peers=true
fund=false
audit=false

# Configuration pour Expo et React Native
strict-peer-deps=false
prefer-offline=false
timeout=60000

# Cache et installation
cache-min=86400
save-exact=false
save-prefix=^

# Logging
loglevel=warn
